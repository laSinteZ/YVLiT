{
  "manifest_version": 3,
  "name": "YVLiT",
  "version": "0.1",
  "description": "Prepend youtube video title with length",
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/watch?v=*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
